    include           <%= @mime_types %>;
    <% if @access_log -%>access_log        <%= @logdir %>/access.log;<% end -%>
    sendfile          <% if @sendfile -%>on<% else -%>off<% end -%>;
    tcp_nopush        <% if @tcp_nopush -%>on<% else -%>off<% end -%>;
    keepalive_timeout <%= @keepalive_timeout %>;
    tcp_nodelay       <% if @tcp_nodelay -%>on<% else -%>off<% end -%>;
    default_type      <%= @default_type %>;
    <% if @gzip -%>
    gzip              on;
    gzip_disable      "<%= @gzip_disable %>";
    gzip_static       <% if @gzip_static -%>on<% else -%>off<% end -%>;
    gzip_comp_level   <%= @gzip_comp_level %>;
    gzip_proxied      <%= @gzip_proxied %>;
    gzip_types        <%= @gzip_types %>;
    <% end -%>

    server_names_hash_bucket_size <%= @server_names_hash_bucket_size %>;

